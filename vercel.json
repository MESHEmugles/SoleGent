{
    "version": 2,
    "builds": [
        {
            "src": "cores/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "base/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/base/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cores/wsgi.py"
        }
    ]
}